    "t = int(input())\n",
    "for i in range(0,t):\n",
    "    N,M = map(int,input().split())\n",
    "    A = list(map(int,input().split()))\n",
    "    te = 0\n",
    "    for j in range(N):\n",
    "        temp = abs(A[j]-1)\n",
    "        tem = abs(A[j]-M)\n",
    "        te += max(temp,tem)\n",
    "    print(te)"
