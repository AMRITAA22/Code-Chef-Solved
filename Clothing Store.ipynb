    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    st, lt, xt, A, B, C = map(int,input().split())\n",
    "    count = 0\n",
    "    count += min(C, xt)\n",
    "    lt += xt - min (C, xt)\n",
    "    count += min(B, lt)\n",
    "    st += lt- min (B,lt)\n",
    "    count += min(A, st)\n",
    "    print(count)"
   ]
