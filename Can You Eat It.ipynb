   "source": [
    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n,k=map(int,input().split())\n",
    "    if n == 0:\n",
    "        print(0)\n",
    "    elif n < k:\n",
    "        print(-1)\n",
    "    elif n % k == 0:\n",
    "        print(n // k)\n",
    "    else:\n",
    "        print(-1)"
   ]
