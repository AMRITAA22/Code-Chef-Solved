    "    x,y,z=map(int,input().split())\n",
    "    total=x*y\n",
    "    if total<=z:\n",
    "        print(0)\n",
    "    else:\n",
    "        c=0\n",
    "        while not total<=z:\n",
    "            total-=y\n",
    "            c+=1\n",
    "        print(c)"
