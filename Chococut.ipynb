   "source": [
    "# cook your dish here\n",
    "from math import ceil\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n,m,k=map(int,input().split())\n",
    "    nside = n - ceil(k/m)      \n",
    "    mside = m - ceil(k/n)      \n",
    "    print(max(n*mside, m*nside))"
   ]
  }
