    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n,a,b=map(int,input().split())\n",
    "    arr = list(map(int, input().split()))\n",
    "    count_a = arr.count(a)\n",
    "    count_b = arr.count(b)\n",
    "    prob_a = count_a / n\n",
    "    prob_b = count_b / n\n",
    "    print(f\"{prob_a * prob_b:.10f}\")"
   ]
