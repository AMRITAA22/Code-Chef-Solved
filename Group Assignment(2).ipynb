    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    p = list(map(int,input().split()))\n",
    "    groups = set(p)\n",
    "    for g in groups:\n",
    "        if p.count(g) % g != 0:\n",
    "            print('NO')\n",
    "            break\n",
    "    else:\n",
    "        print('YES')"
