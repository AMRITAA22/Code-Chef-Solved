    "def solve():\n",
    "    n, x = map(int, input().split())\n",
    "    ages = list(map(int, input().split()))\n",
    "    \n",
    "    eligible_voters = 0\n",
    "    for age in ages:\n",
    "        if age >= x:\n",
    "            eligible_voters += 1\n",
    "    print(eligible_voters)\n",
    "\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    solve()"
