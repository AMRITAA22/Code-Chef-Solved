   "source": [
    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n=int(input())\n",
    "    h=list(map(int,input().split()))\n",
    "    sec = 0\n",
    "    for i in range(n-1):\n",
    "        if h[i] >= h[n-1]:\n",
    "            sec = n - (i + 1)\n",
    "            break\n",
    "    print(sec)"
   ]
