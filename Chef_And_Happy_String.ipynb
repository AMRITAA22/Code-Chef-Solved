   "source": [
    "t = int(input())\n",
    "\n",
    "while t > 0:\n",
    "    s = input()\n",
    "    happy=False\n",
    "    for i in range(len(s)-2):\n",
    "        if s[i] in 'aeiou' and s[i+1] in 'aeiou' and s[i+2] in 'aeiou':\n",
    "            happy=True\n",
    "            break\n",
    "    print(\"Happy\"if happy else \"Sad\")\n",
    "\n",
    "    t -= 1\n"
   ]
