    "# cook your dish here\n",
    "def is_valid_binary_string(N, K):\n",
    "    if K > (N + 1) // 2 or K < (N // 2):\n",
    "        return \"NO\"\n",
    "    return \"YES\"\n",
    "\n",
    "T = int(input())\n",
    "for _ in range(T):\n",
    "    N, K = map(int, input().split())\n",
    "    print(is_valid_binary_string(N, K))"
   ]
  }
