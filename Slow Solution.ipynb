    "# cook your dish here\n",
    "for _ in range(int(input())):\n",
    "    a,b,c=map(int,input().split())\n",
    "    d=c//b\n",
    "    e=int(c%b)\n",
    "    if(d<a):\n",
    "        print((d*b*b)+(e*e))\n",
    "    else:\n",
    "        print(a*b*b)"
