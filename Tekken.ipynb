    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    a,b,c=map(int,input().split())\n",
    "    min3=min(b,c)\n",
    "    p=b-min3\n",
    "    q=c-min3\n",
    "    sum=p+q\n",
    "    if a!=0 and a>sum:\n",
    "        print(\"Yes\")\n",
    "    else:\n",
    "        print(\"No\") "
