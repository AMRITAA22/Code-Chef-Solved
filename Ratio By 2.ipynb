   "source": [
    "# cook your dish here\n",
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    X, Y = map(int, input().split())\n",
    "    d = max(X, Y)\n",
    "    p = min(X, Y)\n",
    "    \n",
    "    op = max(0, p - (d // 2))\n",
    "    ap = max(0, 2 * p - d)\n",
    "    \n",
    "    print(min(ap, op))"
   ]
  }
 ],
