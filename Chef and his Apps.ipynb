    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    s,x,y,z=map(int,input().split())\n",
    "    if (x+y)+z<=s:\n",
    "        print(\"0\")\n",
    "    elif (x+z)<=s or (y+z)<=s:\n",
    "        print(\"1\")\n",
    "    else:\n",
    "        print(\"2\")"
