    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    m,h=map(int,input().split())\n",
    "    BMI=m/(h**2)\n",
    "    if BMI<=18:\n",
    "        print(\"1\")\n",
    "    elif 19<=BMI<=24:\n",
    "        print(\"2\")\n",
    "    elif 25<=BMI<=29:\n",
    "        print(\"3\")\n",
    "    else:\n",
    "        print(\"4\")"
   ]
