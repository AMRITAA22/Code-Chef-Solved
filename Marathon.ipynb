    "    elif n<21: return a \n",
    "    elif n<42: return b \n",
    "    else: return c \n",
    "\n",
    "for i in range(int(input())):\n",
    "    D,d,a,b,c = map(int,input().split())\n",
    "    dis = D*d\n",
    "    ans = prize(dis,a,b,c)\n",
    "    print(ans)"
