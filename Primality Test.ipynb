    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n=int(input())\n",
    "    m=0\n",
    "    for i in range(1,n+1):\n",
    "        if (n%i)==0:\n",
    "            m=m+1\n",
    "    if m==2:\n",
    "        print(\"yes\")\n",
    "    else:\n",
    "        print(\"no\")"
