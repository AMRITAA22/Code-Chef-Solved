    "# cook your dish here\n",
    "import math\n",
    "\n",
    "def sol():\n",
    "    n,x=map(int,input().split())\n",
    "    \n",
    "    if n<=x:\n",
    "        print(0)\n",
    "    else:\n",
    "        needed_candies = n - x\n",
    "        packets = math.ceil(needed_candies / 4)\n",
    "        print(packets)\n",
    "\n",
    "\n",
    "\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    sol()"
