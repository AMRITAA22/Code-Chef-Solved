    "# cook your dish here\n",
    "t=int(input())\n",
    "for _ in range(t):\n",
    "    n,x,k=map(int,input().split())\n",
    "    girls=n-x\n",
    "    remaining_boys = x % k\n",
    "    remaining_girls = girls % k\n",
    "    reading = abs(remaining_boys - remaining_girls)\n",
    "    print(reading)\n"
   ]
