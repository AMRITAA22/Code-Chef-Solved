    "# cook your dish here\n",
    "from math import gcd\n",
    "for _ in range(int(input())):\n",
    "    x,y =map(int, input().split())\n",
    "    if gcd(x, y) > 1:\n",
    "        print(0)\n",
    "    else:\n",
    "        if gcd(x+1, y) > 1 or gcd(y+1, x) > 1:\n",
    "            print(1)\n",
    "        else:\n",
    "            print(2)"
   ]
