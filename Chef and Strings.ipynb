    "# cook your dish here\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    l = list(map(int, input().split()))\n",
    "    skip = 0\n",
    "    for i in range(1,n):\n",
    "        skip = skip + abs(l[i]-l[i-1]) - 1\n",
    "    print(skip)"
